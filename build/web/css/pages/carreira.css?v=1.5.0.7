#carreira .col-md-3, #carreira .col-md-9
{
    padding: 0;
}

#carreira .col-md-9
{
    padding-left: 55px;
}

#carreira .thumbnail
{
    border: none;
    padding: 0;
    margin-bottom: 10px;
}

#carreira h1
{
    text-transform: uppercase;
    font-weight: bold;
    font-family: 'Open Sans Condensed';
    margin: 0;
    font-size: 46px;
    margin-bottom: 10px;
    margin-top: 10px;
    border-bottom: 1px solid #f0f3f5;
}

#carreira h2
{
    font-weight: 400;
    font-family: 'Open Sans Condensed';
    color: #808080;
    font-size: 25px;
    margin: 0;
}

#carreira h3
{
    font-weight: 600;
    font-family: 'Open Sans Condensed';   
    font-size: 24px;
    margin-bottom: 10px;
}

#carreira .descripcion
{
    background-color: #f6f6f6;
    padding: 20px;
}

#carreira .distancia
{
    margin-bottom: 10px;
    font-size: 12px;
    border-bottom: 1px solid #f0f3f5;
}

#carreira table
{
    border-collapse: separate;
    border-spacing: 0px 4px;
}

#carreira tr, #carreira th, #carreira td
{
    border: 0;
    padding: 0;
}

#carreira th
{
    padding: 8px;
}

#carreira td span
{
    padding: 8px;
    display: inline-block;
    width: 100%;
    color: #009adc;
    font-weight: 400;
    font-size: 12px;
}

#carreira td span i
{
    color: #000;
}

#carreira .name
{
    width: 220px;
    border-left: 3px solid #009adc;
    color: #000;
    text-transform: uppercase;
    font-weight: bold;
}

#carreira .inscritos
{
    background-color: #038d34;
    color: #fff;
    padding: 2px;
    margin-top: 6px;
}

#carreira .inscritos i
{
    color: #fff;
}

#carreira .totalinscritos
{
    background-color: #009adc;
    color: #fff;
    padding: 5px;
    margin-top: 6px;
    font-size: 12px;
    text-align: center;
}

#carreira .totalinscritos i
{
    color: #fff;
}

#carreira .plazas
{
    background-color: #009adc;
    color: #fff;
    text-align: center;
    width: 50px;
    padding: 3px;
    font-size: 10px;
}

#carreira .no-plazas
{
    visibility: hidden;
}


#carreira .btn-inscripcion
{
    background-color: #030025;
    color: #fff;
    font-family: 'Open Sans Condensed';
    text-transform: uppercase;
    font-weight: bold;
    border-radius: 0;
    margin-bottom: 5px;
}

#carreira .btn-inscripcion-circuito
{
    margin-left: 5px;
}

#carreira .lb-inscripcion
{
   background-color: #030025;
width: 70%;
color: #fff;
font-family: 'Open Sans Condensed';
text-transform: uppercase;
font-weight: bold;
border-radius: 0;
margin-bottom: 5px;
cursor: default;
font-size: 14px;
padding: 9px;
text-align: center;
}

#carreira .btn-reglamento
{
    background-color: #009adc;
    width: 100%;
    color: #fff;
    font-family: 'Open Sans Condensed';
    text-transform: uppercase;
    font-weight: bold;
    border-radius: 0;
    margin-bottom: 5px;
}

#carreira .contacto 
{
    margin-top: 10px;
    float: left;
    width: 100%;
}

#carreira #webDiv, #carreira #mailDiv
{
    background-color: #009adc;
    font-family: 'Open Sans Condensed';
    text-transform: uppercase;
    color: #fff;
    height: 30px;
    font-weight: bold;
    width: 45%;
}

#carreira #webDiv #web, #carreira #mailDiv #mail 
{
    color: #fff;
    display: inline-block;
    height: 30px;
}

#carreira #webDiv i, #carreira #mailDiv i
{
    float: right;
    padding-top: 5px;
    padding-right: 5px;
    font-size: 20px;
    border-left: 1px solid #fff;
    padding-left: 10px;
    height: 30px;
}

#carreira #webDiv a, #carreira #mailDiv a
{
    padding-top: 5px;
    padding-left: 5px;
    width: 60%;
}

#carreira #webDiv a:hover, #carreira #mailDiv a:hover
{
    text-decoration: none;
}

#carreira #webDiv
{
    float: left;
}

#carreira #mailDiv
{
    float: right;
}

#carreira .contador 
{
    float: left;
    margin-top: 20px;
    width: 100%;
    margin-bottom: 20px;
}

#carreira .datos 
{
    float: left;
}

#carreira .inscritos a
{
    color: #fff;
}

.xdsoft_flipcountdown
{
    width: 222px;
    margin: 0 auto;
    display: block !important;
    margin-bottom: 10px;
}

.counter-label
{
    text-align: center;
}

#counterHeaders
{
    text-align: center;
    font-size: 12px;
}

.video
{
    margin-top: 20px;
    float: left;
}

.recorrido {
    float: left;
    margin-top: 20px;
}

.recorrido img {
    width: 285px;
}

#externa {
    width:30%;
}

#dorsal0 .btn-inscripcion
{
    width: 100%;
}

#comida .btn-inscripcion
{
    width: 100%;
}


#noticias ul {
    margin-top: 10px;
    padding: 0;
}

#noticias li {
    background-color: #f6f6f6;
    border-left: 3px solid #3097da;
    padding: 5px;
    list-style: none;
    margin-top: 10px;
}

#galeria ul
{
    padding: 0;
    margin: 0;
    margin-top: 10px;
}

#galeria li
{
    list-style: none;
    display: inline-block;
    padding: 5px;
}

#resultadoExterno {
    margin-top: 20px;
}

#resultadoExterno
{
    clear: both;
}

#carreira .nav-tabs i
{
    margin-right: 10px;
}

@media (max-width: 436px)
{
    #carreira .col-md-9
    {
        padding-left: 0;
    }
}


@media (min-width: 437px) and (max-width: 767px)
{
    #carreira .col-md-9
    {
        padding-left: 0;
    }
}

i
{
    margin-right: 10px;
}

#resultadoExterno
{
    text-align: center;
}

#dorsal0 {
    text-align: center;
}

#lbRecaudado {
    padding: 5px;
    margin-top: 5px;
    display: inline-block;
}